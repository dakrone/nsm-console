#	| sed 's/002[ :].*/002/' | sort | uniq -c | grep -v '002\\003' | grep -v '002\['
#      1 :\002
#      2 :\002Alvo dos Pacotes\002
#      1 :\002Conectando-se em\002
#      1 :\002Mdia de envio\002
